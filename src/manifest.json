{
  "manifest_version": 3,
  "name": "R10.net Link Düzenleyici",
  "version": "1.1",
  "description": "R10.net sitesindeki yönlendirme linklerini düzenler.",
  "host_permissions": [
    "https://www.r10.net/*"
  ],
  "icons": {
    "48": "icon.png",
    "128": "icon.png"
  },
  "content_scripts": [
    {
      "matches": [
        "https://www.r10.net/*"
      ],
      "js": ["work.js"]
    }
  ]
}